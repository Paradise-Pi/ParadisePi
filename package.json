{
  "name": "ParadisePi",
  "version": "1.0.2",
  "description": "Raspberry Pi based sACN lighting/OSC sound facility control panel",
  "main": "main.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint ."
  },
  "keywords": [],
  "repository": {
    "type": "git",
    "url": "git@github.com:Jbithell/ParadisePi"
  },
  "engines": {
    "npm": ">=8.0.0",
    "node": ">=16.0.0"
  },
  "author": {
    "name": "James Bithell",
    "email": "Jbithell@users.noreply.github.com"
  },
  "license": "GPL-3.0-only",
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "paradisepi"
          }
        },
        {
          "name": "@electron-forge/maker-zip"
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {
            "options": {
              "maintainer": "James Bithell",
              "homepage": "https://github.com/Jbithell/ParadisePi"
            }
          }
        },
        {
          "name": "@electron-forge/maker-dmg",
          "config": {}
        }
      ]
    }
  },
  "dependencies": {
    "better-sqlite3": "^7.5.1",
    "e131": "^1.1.3",
    "electron-squirrel-startup": "^1.0.0",
    "formidable": "^1.2.3",
    "ip": "^1.1.5",
    "jquery": "^3.6.0",
    "knex": "^1.0.4",
    "node-static": "^0.7.11",
    "osc": "^2.4.2",
    "socket.io": "3.1.2"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.63",
    "@electron-forge/maker-deb": "^6.0.0-beta.63",
    "@electron-forge/maker-rpm": "^6.0.0-beta.63",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.63",
    "@electron-forge/maker-zip": "^6.0.0-beta.63",
    "electron": "18.0.4",
    "eslint": "^8.13.0",
    "eslint-config-google": "^0.14.0"
  }
}
